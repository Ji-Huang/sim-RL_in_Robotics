<Configuration>
  <CommunicationType>GRPC</CommunicationType>
  <PortNumber>9092</PortNumber>
  <IPAddress>127.0.0.1</IPAddress>
  <TimestepDurationInSeconds>0.02</TimestepDurationInSeconds>
  <PhyscsSimulationIncrementInSeconds>0.02</PhyscsSimulationIncrementInSeconds>
  <EnableObservationImage updated="yes">false</EnableObservationImage>
  <SaveObservationImageAsFile>false</SaveObservationImageAsFile>
  <ObservationImageWidth updated="yes">1800</ObservationImageWidth>
  <ObservationImageHeight updated="yes">1080</ObservationImageHeight>
  <ObservationImageEncoding>JPG</ObservationImageEncoding>
  <ObservationImageQuality updated="yes">100</ObservationImageQuality>
  <ObservationCameras>
    <CameraParameters>
      <CameraPosition>
        <float updated="yes">0.0</float>
        <float updated="yes">1.0</float>
        <float updated="yes">1.6</float>
      </CameraPosition>
      <CameraRotation>
        <float updated="yes">135.0</float>
        <float updated="yes">0.0</float>
        <float updated="yes">180</float>
      </CameraRotation>
    </CameraParameters>
  </ObservationCameras>
  <EnableGrayscale>false</EnableGrayscale>
  <Items>
    <ItemParameters>
      <ItemType>CUBE</ItemType>
      <ItemMaterial>HETEROGENEOUS</ItemMaterial>
      <ItemObservability>true</ItemObservability>
      <ItemSize>
        <x>0.2</x>
        <y>0.2</y>
        <z>0.3</z>
      </ItemSize>
      <VisualizeItemMaterial>true</VisualizeItemMaterial>
      <ItemMaterialGridX>
        <float>1</float>
      </ItemMaterialGridX>
      <ItemMaterialGridY>
        <float>1</float>
      </ItemMaterialGridY>
      <ItemMaterialGridZ>
        <float>0.5</float>
        <float>0.5</float>
      </ItemMaterialGridZ>
      <ItemMaterialGridDynamicFriction>
        <float>0.01</float>
        <float>0.01</float>
      </ItemMaterialGridDynamicFriction>
      <ItemMaterialGridStaticFriction>
        <float>0.01</float>
        <float>0.01</float>
      </ItemMaterialGridStaticFriction>
      <ItemMass>6</ItemMass>
      <RandomizeItemMass>false</RandomizeItemMass>
      <ItemMassRandomizationRange>0.1</ItemMassRandomizationRange>
      <ItemCenterOfMass>
        <x>0</x>
        <y>0</y>
        <z>0</z>
      </ItemCenterOfMass>
      <RandomizeItemCenterOfMass>false</RandomizeItemCenterOfMass>
      <ItemCenterOfMassRandomizationRange>
        <x>0</x>
        <y>0</y>
        <z>0.075</z>
      </ItemCenterOfMassRandomizationRange>
      <RandomizeItemFriction>false</RandomizeItemFriction>
      <ItemDynamicFrictionRandomizationRange>0.1</ItemDynamicFrictionRandomizationRange>
      <ItemStaticFrictionRandomizationRange>0.1</ItemStaticFrictionRandomizationRange>
      <ItemDrag>2</ItemDrag>
    </ItemParameters>
  </Items>
  <EnableEndEffector>true</EnableEndEffector>
  <EndEffectorModel>CALIBRATION_PIN</EndEffectorModel>
  <MaxJointVelocity>57.29578</MaxJointVelocity>
  <RandomSeed>256</RandomSeed>
  <Evaluation>false</Evaluation>
  <RandomizeAppearance>false</RandomizeAppearance>
  <RandomizeEnvironmentPhysics>false</RandomizeEnvironmentPhysics>
  <CameraPositionRandomizationRangeInMeters>0.05</CameraPositionRandomizationRangeInMeters>
  <CameraRotationRandomizationRangeInDegrees>3</CameraRotationRandomizationRangeInDegrees>
  <EnableShadows>true</EnableShadows>
  <ShadowType>Soft</ShadowType>
  <RandomizeLatency>false</RandomizeLatency>
  <RandomizeTorque>false</RandomizeTorque>
  <Floor>
    <FloorType>MONOCHROMATIC</FloorType>
    <FloorMaterial>HOMOGENEOUS</FloorMaterial>
    <VisualizeFloorMaterial>false</VisualizeFloorMaterial>
    <FloorMaterialGridX>
      <float>1</float>
    </FloorMaterialGridX>
    <FloorMaterialGridY>
      <float>1</float>
    </FloorMaterialGridY>
    <FloorMaterialGridZ>
      <float>1</float>
    </FloorMaterialGridZ>
    <FloorMaterialGridDynamicFriction>
      <float>1</float>
    </FloorMaterialGridDynamicFriction>
    <FloorMaterialGridStaticFriction>
      <float>1</float>
    </FloorMaterialGridStaticFriction>
  </Floor>
  <TrajectoryString />
</Configuration>